<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiazu.web.shop.dao.TestDao">
	<select id="getTest" parameterType="int" resultType="map">
		select * from `t_idvalue` where id = #{id}
	</select>
	<insert id="insertTest" parameterType="map">
  		INSERT INTO `t_idvalue` (`value`) VALUES (#{value})
	</insert>
</mapper>